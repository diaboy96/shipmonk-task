{
  "name": "martindavidik/shipmonk-task",
  "description": "A PHP library that implements a sorted linked list for int or string values.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.3"
  },
  "autoload": {
    "psr-4": {
      "SortedList\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SortedList\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2.2"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
